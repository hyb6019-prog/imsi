package kr.co.iei.photo.model.service;

import java.util.HashMap;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import kr.co.iei.photo.model.dao.PhotoDao;
import kr.co.iei.photo.model.vo.Photo;

@Service
public class PhotoService {
	
	@Autowired
	private PhotoDao photoDao;
	
	@Transactional
	public int insertPhoto(Photo p) {
		int result = photoDao.insertPhoto(p);
		return result;
	}

	public int selectTotalCount() {
		int totalCount = photoDao.selcetTotalCount();
		return totalCount;
	}

	public static List selectPhotoList(int start, int amount) {
		int end = start + amount -1;
		HashMap<String, Object> param = new HashMap<String, Object>();
		param.put("start", start);
		param.put("end", end);
		return null;
	}
}
